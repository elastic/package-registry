  "policy_templates": [
    {
      "name": "cassandra",
      "title": "Cassandra logs and metrics",
      "description": "Collect logs and metrics from Cassandra",
      "inputs": [
        {
          "type": "logfile",
          "title": "Collect System logs from Cassandra",
          "description": "Collecting System logs from Cassandra"
        },
        {
          "type": "jolokia/metrics",
          "vars": [
            {
              "name": "hosts",
              "type": "text",
              "title": "Hosts",
              "multi": true,
              "required": true,
              "show_user": true,
              "default": [
                "localhost:8778"
              ]
            },
            {
              "name": "path",
              "type": "text",
              "title": "Path",
              "multi": false,
              "required": true,
              "show_user": false,
              "default": "/jolokia/?ignoreErrors=true&canonicalNaming=false"
            },
            {
              "name": "username",
              "type": "text",
              "title": "Username",
              "multi": false,
              "required": false,
              "show_user": false,
              "default": "admin"
            },
            {
              "name": "password",
              "type": "password",
              "title": "Password",
              "multi": false,
              "required": false,
              "show_user": false,
              "default": "admin"
            }
          ],
          "title": "Collect Cassandra metrics",
          "description": "Collecting metrics from Cassandra using jolokia"
        }
      ],
      "multiple": true
    }
  ],
  "data_streams": [
    {
      "type": "logs",
      "dataset": "cassandra.log",
      "title": "Cassandra System Logs",
      "release": "ga",
      "ingest_pipeline": "default",
      "streams": [
        {
          "input": "logfile",
          "vars": [
            {
              "name": "paths",
              "type": "text",
              "title": "Paths",
              "multi": true,
              "required": true,
              "show_user": true,
              "default": [
                "/opt/cassandra*/data/system.log"
              ]
            }
          ],
          "template_path": "log.yml.hbs",
          "title": "Cassandra logs",
          "description": "Collect Cassandra system logs",
          "enabled": true
        }
      ],
      "package": "cassandra",
      "path": "log"
    },
    {
      "type": "metrics",
      "dataset": "cassandra.metrics",
      "title": "metrics",
      "release": "ga",
      "ingest_pipeline": "default",
      "streams": [
        {
          "input": "jolokia/metrics",
          "vars": [
            {
              "name": "period",
              "type": "text",
              "title": "Period",
              "multi": false,
              "required": true,
              "show_user": true,
              "default": "10s"
            }
          ],
          "template_path": "stream.yml.hbs",
          "title": "Cassandra metrics",
          "description": "Collect cassandra metrics using jolokia agent",
          "enabled": true,
          "ingestion_method": "Jolokia"
        }
      ],
      "package": "cassandra",
      "path": "metrics"
    }
  ]
